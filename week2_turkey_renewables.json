{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Renewables",
    "anchor": "start",
    "subtitle": "Output, GWh, | IEA via ECO-API",
    "fontSize": "14",
    "subtitleFontSize": "12",
    "color": "#000000",
    "subtitleColor": "#000000"
  },
  "height": "250",
  "width": "container",
  "autosize": {"type": "fit", "contains": "padding"},
  "background": "#ffffff",
  "view": {"fill": "#ffffff", "strokeOpacity": 0},
  "data": {
    "url": "https://api.economicsobservatory.com/tur/rGWh?vega",
    "format": {"type": "json"}
  },
  "mark": {"type": "bar", "color": "#f4134d", "strokeWidth": "2"},
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    }
  }
}